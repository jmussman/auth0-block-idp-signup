    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Test",
            "request": "launch",
            "type": "node",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "runtimeArgs": [
                "run",
                "test"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Run Test-Coverage",
            "request": "launch",
            "type": "node",
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "runtimeArgs": [
                "run",
                "test-coverage"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}